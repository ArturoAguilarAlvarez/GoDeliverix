                                            <div class="form-group">
                                                <asp:LinkButton CssClass="btn btn-sm btn-danger" Style="border-radius: 50px;" ID="btnQuita" OnClick="btnQuita_Click" runat="server">
                                                                <span class="glyphicon glyphicon-minus"></span>

    <link href="../../Content/Loader.css" rel="stylesheet" />
    <script type="text/javascript">
        $(function () {
            showPopup = function () {

                $("#myModal").modal('show');
                return true;
            }
            MuestraSucursales = function () {

                $("#ModalS").modal('show');
                return true;
            }

            MuestraProducto = function () {

                $("#ModalProducto").modal('show');
                return true;
            }
            OcultaProducto = function () {

                $("#ModalProducto").modal('hide');
                return true;
            }
            OcultaSucursales = function () {
                $("#ModalS").modal('hide');
                return true;
            }
            closePopup = function () {
                $("#myModal").modal('hide');
                return true;
            }


            MuestraCarrito = function () {
                $("#modalCarrito").modal('show');
                return true;
            }

            OcultaModalEliminarproducto = function () {
                $("#ModalEliminarProductoCarrito").modal('hide');
                return true;
            }

        });
    </script>
</head>

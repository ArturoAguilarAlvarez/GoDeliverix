                                                        <h4>
                                                            <asp:Label ID="lblIdentificador" runat="server" /></h4>
                                                        <strong>
                                                            <h6>
                                                                <asp:Label ID="lblColonia" runat="server" />
                                                            </h6>
                                                        </strong>
                                                    </div>

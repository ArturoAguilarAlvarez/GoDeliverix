<Window x:Class="DeliverixSucursales.LicenciaRequerida"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DeliverixSucursales"
        mc:Ignorable="d"
        Title="Aviso" ResizeMode="NoResize" WindowStartupLocation="CenterScreen"  Width="800" Height="200" WindowStyle="None">
    <Grid>
        <TextBlock x:Name="lblMensaje" Text="Licencia Requerida!!!"  FontSize="20"  HorizontalAlignment="Center" TextAlignment="Center" VerticalAlignment="Top" Margin="10 20 0 0" Height="70" Width="600"/>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">

            <Button x:Name="btnAceptar" Click="btnAceptar_Click" Content="Aceptar" HorizontalAlignment="Center" Height="30" Margin="0 20 0 0" Background="Green"  VerticalAlignment="Center" Width="96"/>
        </StackPanel>
    </Grid>
</Window>

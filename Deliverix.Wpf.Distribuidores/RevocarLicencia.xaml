<Window x:Class="DeliverixSucursales.RevocarLicencia"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DeliverixSucursales"
        mc:Ignorable="d"
        Title="Revocar Licencia" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" Height="164.602" Width="300">
    <Grid>
        <Label x:Name="lblMensaje" Content="Desea eliminar la licencia existente?" HorizontalAlignment="Center" Height="30" Margin="39,24,41,0" VerticalAlignment="Top" Width="212"/>
        <Button x:Name="btnAceptar" Content="Aceptar" Click="btnAceptar_Click" HorizontalAlignment="Left" Height="26" Margin="37,78,0,0" VerticalAlignment="Top" Width="90" Background="#FF257F03"/>
        <Button x:Name="btnCancelar" Content="Cancelar" Click="btnCancelar_Click" HorizontalAlignment="Left" Height="26" Margin="168,78,0,0" VerticalAlignment="Top" Width="90" Background="#FFF32121"/>

    </Grid>
</Window>
